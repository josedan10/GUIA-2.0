{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "start": "webpack --mode development --env.NODE_ENV=development",
    "test": "webpack --watch --mode development --env.NODE_ENV=development",
    "sass-compiler": "node-sass --include-path scss resources/assets/sass/app.scss public/css/main.css",
    "development": "npm run sass-compiler && npm run test",
    "web-prod": "webpack --mode production --env.NODE_ENV=development",
    "server": "webpack-dev-server --mode development --open",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production"
  },
